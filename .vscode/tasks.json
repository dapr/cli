{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "build-debug",
            "type": "process",
            "command": "make",
            "args": [
                "DEBUG=1",
                "build"
            ],
            "problemMatcher": ["$go"],
            "group": {
                "kind": "build",
                "isDefault": true
            }
        },
        {
            "label": "build-release",
            "type": "process",
            "command": "make",
            "args": [
                "build"
            ],
            "problemMatcher": ["$go"]
        }
    ]
}